# dotfiles
#
# Subversion Aliases

alias s="svn"
alias sa="svn add"
alias srm="svn delete"
alias smv="svn move"
alias sco="svn checkout"
alias sci="svn commit -m"
alias slog="svn log | less"
alias sdiff="svn diff | mate"
alias sup="svn update"
alias supi="svn update --ignore-externals"
alias sst="clear; svn status"
alias ssti="clear; svn status --ignore-externals"
alias svnclean="find . -name .svn -exec rm -rf {} \;"
